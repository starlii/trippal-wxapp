/* pages/narrator/narrator.wxss */
.narrator__header {
  background-size: 100% 344rpx;
  background-image: url('data:image/jpeg;base64,');
  background-color: #258dfe;
  background-repeat: no-repeat;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 30rpx;
  position: relative;
}
.listen {
  position: absolute;
  top: 10rpx;
  right: 24rpx;
  width: 48rpx;
  height: 48rpx;
}

.narrator-avatar {
  width: 118rpx;
  height: 118rpx;
  border-radius: 50%;
  border: 4rpx solid #53a3fa;
  margin-top: 10rpx;
  margin-bottom: 15rpx;
}
.narrator-name {
  font-size: 30rpx;
  color: #ffffff;
  margin-bottom: 20rpx;
}
.info-item {
  border-radius: 10px;
  background-color: #f6fb72;
  font-size: 20rpx;
  color: #343434;
  height: 36rpx;
  line-height: 36rpx;
  padding: 3rpx 14rpx;
  display: flex;
  align-items: center;
}
.info-item image {margin-right: 10rpx}
.info-item+.info-item {
  margin-left: 10rpx;
}

.intro {
  padding: 0 24rpx;
  overflow-wrap: break-spaces;
  font-size: 20rpx;
  color: #ffffff;
  text-indent: 40rpx;
}

.album-title {
  height: 72rpx;
  line-height: 72rpx;
  padding: 0 24rpx;
  font-size: 32rpx;
  color: #343434;
  background-color: #ffffff;
  border-bottom: 2px solid #f6f6f6;
}